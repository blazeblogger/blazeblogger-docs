#. Tag: title
#, no-c-format
msgid "<command>blaze-log</command>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "<command>blaze-log</command> displays the &PRODUCT; repository log."
msgstr ""

#. Tag: title
#, no-c-format
msgid "Synopsis"
msgstr ""

#. Tag: screen
#, no-c-format
msgid ""
"<command>blaze-log</command> <optional><option>-cqrsCV</option></optional> <optional><option>-b</option> <replaceable>directory</replaceable></optional> <optional><option>-n</option> <replaceable>number</replaceable></optional>\n"
"\n"
"<command>blaze-log</command> <option>-h</option>|<option>-v</option>"
msgstr ""

#. Tag: title
#, no-c-format
msgid "Description"
msgstr ""

#. Tag: para
#, no-c-format
msgid "<command>blaze-log</command> displays the content of the &PRODUCT; repository log."
msgstr ""

#. Tag: title
#, no-c-format
msgid "Options"
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>-b</option> <replaceable>directory</replaceable>, <option>--blogdir</option> <replaceable>directory</replaceable>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Allows you to specify a <replaceable>directory</replaceable> in which the &PRODUCT; repository is placed. The default option is a current working directory."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>-n</option> <replaceable>number</replaceable>, <option>--number</option> <replaceable>number</replaceable>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Allows you to specify a <replaceable>number</replaceable> of log entries to be listed."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>-s</option>, <option>--short</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Tells <command>blaze-log</command> to display each log entry on a single line."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>-r</option>, <option>--reverse</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Tells <command>blaze-log</command> to display log entries in reverse order."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>-c</option>, <option>--color</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Enables colored output. When supplied, this option overrides the relevant configuration option."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>-C</option>, <option>--no-color</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Disables colored output. When supplied, this option overrides the relevant configuration option."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>-q</option>, <option>--quiet</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Disables displaying of unnecessary messages."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>-V</option>, <option>--verbose</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Enables displaying of all messages. This is the default option."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>-h</option>, <option>--help</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Displays usage information and exits."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>-v</option>, <option>--version</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Displays version information and exits."
msgstr ""

#. Tag: title
#, no-c-format
msgid "Files"
msgstr ""

#. Tag: term
#, no-c-format
msgid "<filename>.blaze/log</filename>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "A file containing the repository log."
msgstr ""

#. Tag: title
#, no-c-format
msgid "Example Usage"
msgstr ""

#. Tag: para
#, no-c-format
msgid "List the whole repository history:"
msgstr ""

#. Tag: screen
#, no-c-format
msgid ""
"~]$ <command>blaze-log</command>\n"
"Date: Sun Jul 25 16:48:22 2010\n"
"\n"
"    Edited the page with ID 5.\n"
"\n"
"Date: Tue Jul  6 18:54:59 2010\n"
"\n"
"    Edited the page with ID 5.\n"
"\n"
"<emphasis>etc.</emphasis>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "List the whole repository history in reverse order:"
msgstr ""

#. Tag: screen
#, no-c-format
msgid ""
"~]$ <command>blaze-log -r</command>\n"
"Date: Tue Feb 10 00:40:16 2009\n"
"\n"
"    Created/recovered a BlazeBlogger repository.\n"
"\n"
"Date: Tue Feb 10 01:06:44 2009\n"
"\n"
"    Added the page with ID 1.\n"
"\n"
"<emphasis>etc.</emphasis>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Display the very first log entry on a single line:"
msgstr ""

#. Tag: screen
#, no-c-format
msgid ""
"~]$ <command>blaze-log -rs -n 1</command>\n"
"Tue Feb 10 00:40:16 2009 - Created/recovered a BlazeBlogger repository."
msgstr ""

