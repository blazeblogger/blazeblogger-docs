#. Tag: title
#, no-c-format
msgid "<command>blaze.bat</command>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "<command>blaze.bat</command> allows you to run all &PRODUCT; utilities with a single command."
msgstr ""

#. Tag: title
#, no-c-format
msgid "Synopsis"
msgstr ""

#. Tag: screen
#, no-c-format
msgid ""
"<command>blaze</command> <replaceable>command</replaceable> <optional><replaceable>option</replaceable>â€¦</optional>\n"
"\n"
"<command>blaze</command> <option>help</option> <optional><replaceable>command</replaceable></optional>"
msgstr ""

#. Tag: title
#, no-c-format
msgid "Description"
msgstr ""

#. Tag: para
#, no-c-format
msgid "<command>blaze.bat</command> provides a common way to run &PRODUCT; utilities with a single command."
msgstr ""

#. Tag: title
#, no-c-format
msgid "Options"
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>init</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Runs <command>blaze-init</command>, a utility that either creates a &PRODUCT; repository, or recovers a previously corrupted one. Refer to <xref linkend=\"sect-Core_Utilities-init\" /> for a full description of the command usage."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>config</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Runs <command>blaze-config</command>, a utility that either displays, or sets &PRODUCT; configuration options. Refer to <xref linkend=\"sect-Core_Utilities-config\" /> for a full description of the command usage."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>add</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Runs <command>blaze-add</command>, a utility that adds a blog post or a page to the &PRODUCT; repository. Refer to <xref linkend=\"sect-Core_Utilities-add\" /> for a full description of the command usage."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>edit</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Runs <command>blaze-edit</command>, a utility that edits a blog post or a page in the &PRODUCT; repository. Refer to <xref linkend=\"sect-Core_Utilities-edit\" /> for a full description of the command usage."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>remove</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Runs <command>blaze-remove</command>, a utility that removes a blog post or a page from the &PRODUCT; repository. Refer to <xref linkend=\"sect-Core_Utilities-remove\" /> for a full description of the command usage."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>list</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Runs <command>blaze-list</command>, a utility that lists blog posts or pages in the &PRODUCT; repository. Refer to <xref linkend=\"sect-Core_Utilities-list\" /> for a full description of the command usage."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>make</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Runs <command>blaze-make</command>, a utility that generates a blog from the &PRODUCT; repository. Refer to <xref linkend=\"sect-Core_Utilities-make\" /> for a full description of the command usage."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>log</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Runs <command>blaze-log</command>, a utility that displays the &PRODUCT; repository log. Refer to <xref linkend=\"sect-Core_Utilities-log\" /> for a full description of the command usage."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>help</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Displays usage information, either for the utility itself, or for the selected <replaceable>command</replaceable>."
msgstr ""

#. Tag: title
#, no-c-format
msgid "Example Usage"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Run the <command>blaze-remove</command> command:"
msgstr ""

#. Tag: screen
#, no-c-format
msgid "C:\\&gt; <command>blaze remove -p 4</command>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Display usage of the <command>blaze-config</command> command:"
msgstr ""

#. Tag: screen
#, no-c-format
msgid ""
"C:\\&gt; <command>blaze help config</command>\n"
"Usage: blaze-config [-qV] [-b DIRECTORY] [-E EDITOR] OPTION [VALUE...]\n"
"       blaze-config -e [-b DIRECTORY]\n"
"       blaze-config -h|-v\n"
"\n"
"  -b, --blogdir DIRECTORY     specify a directory in which the BlazeBlogger\n"
"                              repository is placed\n"
"  -E, --editor EDITOR         specify an external text editor\n"
"  -e, --edit                  edit the configuration in a text editor\n"
"  -q, --quiet                 do not display unnecessary messages\n"
"  -V, --verbose               display all messages\n"
"  -h, --help                  display this help and exit\n"
"  -v, --version               display version information and exit"
msgstr ""

