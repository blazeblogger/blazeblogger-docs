#. Tag: title
#, no-c-format
msgid "<command>blaze-remove</command>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "<command>blaze-remove</command> removes a blog post or a page from the &PRODUCT; repository."
msgstr ""

#. Tag: title
#, no-c-format
msgid "Synopsis"
msgstr ""

#. Tag: screen
#, no-c-format
msgid ""
"<command>blaze-remove</command> <optional><option>-fipqPV</option></optional> <optional><option>-b</option> <replaceable>directory</replaceable></optional> <replaceable>id</replaceable>â€¦\n"
"\n"
"<command>blaze-remove</command> <option>-h</option>|<option>-v</option>"
msgstr ""

#. Tag: title
#, no-c-format
msgid "Description"
msgstr ""

#. Tag: para
#, no-c-format
msgid "<command>blaze-remove</command> removes a blog post or a page with the specified <replaceable>id</replaceable> from the &PRODUCT; repository."
msgstr ""

#. Tag: title
#, no-c-format
msgid "Options"
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>-b</option> <replaceable>directory</replaceable>, <option>--blogdir</option> <replaceable>directory</replaceable>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Allows you to specify a <replaceable>directory</replaceable> in which the &PRODUCT; repository is placed. The default option is a current working directory."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>-p</option>, <option>--page</option>, <option>--pages</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Tells <command>blaze-remove</command> to remove a page or pages."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>-P</option>, <option>--post</option>, <option>--posts</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Tells <command>blaze-remove</command> to remove a blog post or blog posts. This is the default option."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>-f</option>, <option>--force</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Disables requiring manual confirmation of each blog post or page removal. This is the default option."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>-i</option>, <option>--interactive</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Enables requiring manual confirmation of each blog post or page removal."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>-q</option>, <option>--quiet</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Disables displaying of unnecessary messages."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>-V</option>, <option>--verbose</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Enables displaying of all messages. This is the default option."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>-h</option>, <option>--help</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Displays usage information and exits."
msgstr ""

#. Tag: term
#, no-c-format
msgid "<option>-v</option>, <option>--version</option>"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Displays version information and exits."
msgstr ""

#. Tag: title
#, no-c-format
msgid "Example Usage"
msgstr ""

#. Tag: para
#, no-c-format
msgid "Remove a blog post:"
msgstr ""

#. Tag: screen
#, no-c-format
msgid ""
"~]$ <command>blaze-remove 10</command>\n"
"Successfully removed the post with ID 10."
msgstr ""

#. Tag: para
#, no-c-format
msgid "Remove a page:"
msgstr ""

#. Tag: screen
#, no-c-format
msgid ""
"~]$ <command>blaze-remove -p 4</command>\n"
"Successfully removed the page with ID 4."
msgstr ""

#. Tag: para
#, no-c-format
msgid "Remove multiple blog posts:"
msgstr ""

#. Tag: screen
#, no-c-format
msgid ""
"~]$ <command>blaze-remove 10 4 6</command>\n"
"Successfully removed the post with ID 10, 4 and 6."
msgstr ""

#. Tag: para
#, no-c-format
msgid "Remove multiple blog posts safely:"
msgstr ""

#. Tag: screen
#, no-c-format
msgid ""
"~]$ <command>blaze-remove -i 10 4 6</command>\n"
"Remove the post with ID 10 titled `Debian and Fedora Packages'? <userinput>y</userinput>\n"
"Remove the post with ID 4 titled `BlazeBlogger 0.8.0 RC2'? <userinput>y</userinput>\n"
"Remove the post with ID 6 titled `BlazeBlogger 0.8.1'? <userinput>y</userinput>\n"
"Successfully removed the post with ID 10, 4 and 6."
msgstr ""

